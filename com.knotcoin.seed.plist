<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.knotcoin.seed</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>$HOME/Desktop/KNOTCOIN/target/release/knotcoind</string>
        <string>--p2p-port=9000</string>
        <string>--rpc-port=9001</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
        <key>Crashed</key>
        <true/>
    </dict>
    
    <key>StandardOutPath</key>
    <string>$HOME/.knotcoin/seed.log</string>
    
    <key>StandardErrorPath</key>
    <string>$HOME/.knotcoin/seed.error.log</string>
    
    <key>WorkingDirectory</key>
    <string>$HOME/Desktop/KNOTCOIN</string>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>RUST_LOG</key>
        <string>info</string>
    </dict>
    
    <key>ThrottleInterval</key>
    <integer>10</integer>
    
    <key>Nice</key>
    <integer>-5</integer>
</dict>
</plist>
